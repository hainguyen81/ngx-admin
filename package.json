        "url": "https://github.com/akveo/ngx-admin/issues"
    },
    "scripts": {
        "circular_detect": "npx madge --circular ./",
        "node-patch": "node ./src/app/@patches/crypto-js/patch.js",
        "postinstall-ivy": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
        "postinstall-umd": "ngcc --properties main --create-ivy-entry-points",
        "postinstall": "npm run node-patch & npm run postinstall-ivy & npm run postinstall-umd",
        "ng": "ng",
        "ng-size": "npm run node-patch & npm run unused-css & node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng",
        "ng-size-build": "npm run node-patch & npm run unused-css & node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build",
        "ng-size-start": "npm run node-patch & npm run unused-css & node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng serve",
        "conventional-changelog": "conventional-changelog",
        "start": "npm run ng-size-start",
        "start:proxy": "npm run ng-size-start -- --proxy-config proxy.conf.json",
        "build": "npm run ng-size-build",
        "build:dev": "npm run ng-size-build -- --configuration dev",
        "build:prod": "npm run ng-size-build -- --prod --aot --configuration production --aot",
        "build:github_dev": "npm run ng-size-build -- --configuration github_dev --base-href https://hainguyen81.github.io/ngx-admin/",
        "build:github_prod": "npm run ng-size-build -- --prod --aot --configuration github_prod --base-href https://hainguyen81.github.io/ngx-admin/",
        "publish:to_github": "ngh -S",
        "build_publish:github_dev": "npm run ng-size-build -- --configuration github_dev --base-href https://hainguyen81.github.io/ngx-admin/ & npm run publish:to_github",
        "build_publish:github_prod": "npm run ng-size-build -- --prod --aot --configuration github_prod --base-href https://hainguyen81.github.io/ngx-admin/ & npm run publish:to_github",
        "unused-css": "ngx-unused-css",
        "test": "ng test",
        "test:coverage": "rimraf coverage && npm run test -- --code-coverage",
        "lint": "ng lint",
        "lint:fix": "ng lint ngx-admin-demo --fix",
        "lint:styles": "stylelint ./src/**/*.scss",
        "lint:ci": "npm run lint && npm run lint:styles",
        "pree2e": "webdriver-manager update --standalone false --gecko false",
        "e2e": "ng e2e",
        "docs": "compodoc -p src/tsconfig.app.json -d docs",
        "docs:serve": "compodoc -p src/tsconfig.app.json -d docs -s",
        "release:changelog": "npm run conventional-changelog -- -p angular -i CHANGELOG.md -s"
    },
